{
  "name": "aceinna_imu",
  "title": "Aceinna IMU",
  "description": "Open-source, embedded development platform for Aceinna IMU hardware. Run custom algorithms and navigation code on Aceinna IMU/INS hardware.",
  "homepage": "https://www.aceinna.com",
  "keywords": [
    "dev-platform",
    "navigation",
    "ACEINNA",
    "Inertial Measurement Units",
    "Inertial Navigation Systems",
    "IMU",
    "INS"
  ],
  "license": "Apache-2.0",
  "engines": {
    "platformio": "<5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aceinna/platform-aceinna_imu.git"
  },
  "version": "1.3.0",
  "packageRepositories": [
    "https://dl.bintray.com/platformio/dl-packages/manifest.json",
    "http://dl.platformio.org/packages/manifest.json"
  ],
  "packages": {
    "toolchain-gccarmnoneeabi": {
      "type": "toolchain",
      "version": ">=1.60301.0,<1.80000.0"
    },
    "tool-openocd": {
      "type": "uploader",
      "optional": true,
      "version": "~2.1000.0"
    },
    "tool-jlink": {
      "type": "uploader",
      "optional": true,
      "version": "^1.63208.0"
    }
  },
  "pythonPackages": {
      "openimu": ">=1.0.4,<2"
  }
}
